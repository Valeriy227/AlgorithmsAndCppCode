vec

ll cross_prod(pair<ll, ll> a, pair<ll, ll> b) {
    return a.first * b.second - a.second * b.first;
}

ll scal_prod(pair<ll, ll> a, pair<ll, ll> b) {
    return a.first * b.first + a.second * b.second;
}

int main() {
    int n;
